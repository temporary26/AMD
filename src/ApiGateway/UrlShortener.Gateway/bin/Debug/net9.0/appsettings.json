{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Ocelot": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Gateway": {
    "Name": "UrlShortener.Gateway",
    "Version": "1.0.0",
    "Environment": "Development"
  },
  "DownstreamServices": {
    "UrlShortenerAPI": {
      "BaseUrl": "http://localhost:5199",
      "HealthCheckPath": "/health",
      "TimeoutSeconds": 30
    }
  }
}
